{
  "name": "corkpin",
  "title": "corkpin",
  "description": "Memorial board forever",
  "author": "sideroad",
  "license": "MIT",
  "version": "1.0.0",
  "repository": {},
  "keywords": [
    "corkpin"
  ],
  "main": "bin/server.js",
  "scripts": {
    "start": "better-npm-run prod",
    "build": "better-npm-run build",
    "postinstall": "npm run build",
    "lint": "eslint -c .eslintrc src",
    "dev": "concurrent --kill-others \"better-npm-run watch\" \"better-npm-run dev\""
  },
  "betterScripts": {
    "prod": {
      "command": "node bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production"
      }
    },
    "build": {
      "command": "webpack --verbose --colors --display-error-details --config webpack/prod.config.js",
      "env": {
        "NODE_ENV": "production",
        "GLOBAL_HOST": "corkpin.herokuapp.com",
        "GLOBAL_PORT": 443
      }
    },
    "watch": {
      "command": "node webpack/webpack-dev-server.js",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "NODE_PATH": "./src",
        "GLOBAL_HOST": "localhost",
        "GLOBAL_PORT": "3000"
      }
    },
    "dev": {
      "command": "node bin/server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development",
        "HOST": "localhost",
        "PORT": "3000",
        "GLOBAL_HOST": "localhost",
        "GLOBAL_PORT": "3000"
      }
    }
  },
  "dependencies": {
    "babel-core": "^6.5.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-system-import-transformer": "^2.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-display-name": "^6.3.13",
    "babel-plugin-transform-runtime": "^6.3.13",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.3.13",
    "babel-runtime": "^6.3.19",
    "body-parser": "^1.14.2",
    "cloudinary": "^1.4.6",
    "compression": "^1.6.0",
    "cookie-parser": "^1.4.3",
    "express": "^4.13.3",
    "express-session": "^1.12.1",
    "file-loader": "^0.9.0",
    "fs-extra": "^1.0.0",
    "history": "^2.1.2",
    "is-video": "^1.0.1",
    "isomorphic-fetch": "^2.2.0",
    "jsx-loader": "^0.13.2",
    "koiki": "^4.1.12",
    "koiki-ui": "0.0.32",
    "less": "^2.5.3",
    "less-loader": "^2.2.1",
    "lodash": "^4.17.2",
    "multer": "^1.2.1",
    "passport": "^0.3.2",
    "passport-facebook": "^2.1.1",
    "piping": "^0.3.0",
    "pretty-error": "^1.2.0",
    "query-string": "^3.0.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-dropzone": "^3.8.0",
    "react-fastclick": "^2.1.0",
    "react-helmet": "^2.3.1",
    "react-modal": "^1.5.2",
    "react-redux": "^4.0.0",
    "react-router": "^2.8.1",
    "react-router-redux": "^4.0.4",
